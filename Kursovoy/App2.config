<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8.1" />
	</startup>

	<system.runtime.remoting>
		<application>
			<channels>
				<channel ref="http" port="8085" name="HTTPChannel" />
			</channels>
			<service>
				<wellknown mode="SingleCall" type="RemoteObject.ServerObject, RemoteObject" objectUri="ServerObject.soap" />
			</service>
		</application>
	</system.runtime.remoting>
</configuration>